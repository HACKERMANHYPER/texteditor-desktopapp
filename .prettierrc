{
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 4,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "tailwindStylesheet": "./src/app.css"
}
